<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<!-- *****************************用户管理***************************** -->
	<package name="partner" namespace="/partner" extends="progenitor">
		<action name="*_*" class="partnerAction" method="{2}">
			<result name="provider" type="redirect">/page/partner/provider.jsp</result>
			<result name="consumer" type="redirect">/page/partner/consumer.jsp</result>
		</action>
	</package>
</struts>
